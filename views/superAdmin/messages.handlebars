<h1>Messages</h1>

<table class="table table-bordered">

    <thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Message</th>
        <th>Date</th>
    </tr>

    </thead>

    <tbody>
    {{#each messages}}
        <tr>
            <td>{{name}}</td>
            <td>{{email}}</td>
            <td>{{phone}}</td>
            <td>{{message}}</td>
            <td>{{generateTime date 'MMMM Do YYYY'}}</td>
        </tr>
    {{/each}}
    </tbody>

</table>